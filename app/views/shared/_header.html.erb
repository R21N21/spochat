<header class='header-wrapper'>
  <div class='heading-contents'>
    <%= link_to image_tag("spochat_logo.png", class:"spochat-logo"), "/" %>
    <div class="create-room">
      <%= link_to "ルーム作成", '#', class: "create-room-name" %>
    </div>
   </div>

  
  <div class='nav'>
    <ul class='lists'>
      <%# if user_signed_in? %>
        <li><%= link_to 'name', "#", class: "user-name" %></li>
        <li><%= link_to 'ログアウト', "#", method: :delete, class: "logout" %></li>
      <%# else %>
        <li><%= link_to 'ログイン', "#", class: "login" %></li>
        <li><%= link_to '新規登録', "#", class: "sign-up" %></li>
      <%# end %>
    </ul>
  </div>
</header>